                            <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>
                            <subviews>
                                <button toolTip="For compliance testing, you need to extract the sizes of various components to verify their size is not too " id="226" userLabel="Capture Groups">
                                    <rect key="frame" x="16" y="122" width="848" height="18"/>
                                    <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMinY="YES"/>
                                    <buttonCell key="cell" type="check" title="Create Capture Groups for display-name, local-name, and domain" bezelStyle="regularSquare" imagePosition="left" alignment="left" inset="2" id="227">
                                        <behavior key="behavior" changeContents="YES" doesNotDimImage="YES" lightByContents="YES"/>
